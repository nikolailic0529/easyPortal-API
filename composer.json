{
  "name": "fakharanwar/easyportal-api",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "proprietary",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo_mysql": "*",
    "ext-redis": "*",
    "barryvdh/laravel-snappy": "^0.4.8",
    "dragonmantank/cron-expression": "^3.1",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "h4cc/wkhtmltopdf-amd64": "^0.12.4",
    "jetbrains/phpstorm-attributes": "^1.0",
    "laravel/framework": "^8.12",
    "laravel/tinker": "^2.5",
    "lastdragon-ru/lara-asp-core": "dev-master",
    "lastdragon-ru/lara-asp-eloquent": "dev-master",
    "lastdragon-ru/lara-asp-formatter": "dev-master",
    "lastdragon-ru/lara-asp-graphql": "dev-master",
    "lastdragon-ru/lara-asp-migrator": "dev-master",
    "lastdragon-ru/lara-asp-queue": "dev-master",
    "lastdragon-ru/lara-asp-spa": "dev-master",
    "lcobucci/jwt": "^4.1",
    "league/oauth2-client": "^2.6",
    "maatwebsite/excel": "^3.1",
    "nuwave/lighthouse": "^5.2",
    "phpdocumentor/reflection-docblock": "^5.2",
    "propaganistas/laravel-phone": "^4.2"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.9",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "itsgoingd/clockwork": "^5.0",
    "lastdragon-ru/lara-asp-testing": "dev-master",
    "mll-lab/laravel-graphql-playground": "^2.5",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3",
    "slevomat/coding-standard": "dev-master",
    "squizlabs/php_codesniffer": "dev-master"
  },
  "suggest": {
    "ext-pdo": "Required for laravel/framework",
    "ext-fileinfo": "Required for league/flysystem",
    "ext-intl": "Required for lastdragon-ru/lara-asp-formatter",
    "ext-xml": "Required for lastdragon-ru/lara-asp-testing",
    "ext-libxml": "Required for lastdragon-ru/lara-asp-testing",
    "ext-dom": "Required for lastdragon-ru/lara-asp-testing",
    "ext-xmlreader": "Required for lastdragon-ru/lara-asp-testing"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "nuwave/lighthouse"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Config\\": "config/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "app/mixins.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "ide-helper": [
      "@php artisan ide-helper:meta --filename .phpstorm.meta.php/meta.php",
      "@php artisan ide-helper:generate .phpstorm.meta.php/helper.php",
      "@php artisan ide-helper:models --write",
      "@php artisan lighthouse:ide-helper",
      "@php -r 'file_put_contents(\"schema-directives.graphql\", str_replace(\"repeatable\", \"\", file_get_contents(\"schema-directives.graphql\")));'",
      "@php -r 'rename(\"_lighthouse_ide_helper.php\", \".phpstorm.meta.php/lighthouse.php\");'",
      "@php -r 'rename(\"programmatic-types.graphql\", \".phpstorm.meta.php/lighthouse-types.graphql\");'",
      "@php -r 'rename(\"schema-directives.graphql\", \".phpstorm.meta.php/lighthouse-directives.graphql\");'"
    ],
    "phpunit": [
      "@php ./vendor/bin/phpunit"
    ],
    "phpcs": [
      "@php ./vendor/bin/phpcs"
    ]
  }
}
