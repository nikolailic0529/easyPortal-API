{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "id",
        "given_name",
        "family_name",
        "locale",
        "root"
    ],
    "properties": {
        "id": {
            "$ref": "../types.json#/definitions/ID"
        },
        "given_name": {
            "type": "string"
        },
        "family_name": {
            "type": "string"
        },
        "locale": {
            "type": [
                "string",
                "null"
            ]
        },
        "root": {
            "type": "boolean"
        }
    }
}
