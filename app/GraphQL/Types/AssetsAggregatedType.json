    "type": "object",
    "additionalProperties": false,
    "required": [
        "count",
        "type_id",
        "type"
    ],
    "properties": {
        "count": {
            "type": "integer"
        },
        "type_id": {
            "oneOf": [
                {
                    "$ref": "../types.json#/definitions/ID"
                },
                {
                    "type": "null"
                }
            ]
        },
        "type": {
            "oneOf": [
                {
                    "$ref": "../types.json#/definitions/Type"
                },
                {
                    "type": "null"
                }
            ]
        }
    }
}
