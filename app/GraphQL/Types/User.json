{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionProperties": false,
    "type": "object",
    "required": [
        "given_name",
        "family_name"
    ],
    "properties": {
        "id": {
            "$ref": "../types.json#/definitions/ID"
        },
        "given_name": {
            "type": [
                "string",
                "null"
            ]
        },
        "family_name": {
            "type": [
                "string",
                "null"
            ]
        },
        "email": {
            "type": "string"
        },
        "email_verified": {
            "type": "boolean"
        },
        "enabled": {
            "type": "boolean"
        },
        "role": {
            "oneOf": [
                {
                    "$ref": "../Types/Role.json"
                },
                {
                    "type": "null"
                }
            ]
        },
        "invitations": {
            "type": "array",
            "items": {
                "$ref": "../Types/Invitation.json"
            }
        },
        "team": {
            "oneOf": [
                {
                    "$ref": "../Types/Team.json"
                },
                {
                    "type": "null"
                }
            ]
        },
        "title": {
            "type": [
                "string",
                "null"
            ]
        },
        "academic_title": {
            "type": [
                "string",
                "null"
            ]
        },
        "office_phone": {
            "type": [
                "string",
                "null"
            ]
        },
        "mobile_phone": {
            "type": [
                "string",
                "null"
            ]
        },
        "contact_email": {
            "type": [
                "string",
                "null"
            ]
        },
        "department": {
            "type": [
                "string",
                "null"
            ]
        },
        "job_title": {
            "type": [
                "string",
                "null"
            ]
        },
        "company": {
            "type": [
                "string",
                "null"
            ]
        },
        "phone": {
            "type": [
                "string",
                "null"
            ]
        },
        "photo": {
            "type": [
                "string",
                "null"
            ]
        },
        "locale": {
            "type": [
                "string",
                "null"
            ]
        },
        "timezone": {
            "type": [
                "string",
                "null"
            ]
        },
        "organizations": {
            "type": "array",
            "items": {
                "$ref": "./OrganizationUser.json"
            }
        }
    }
}
