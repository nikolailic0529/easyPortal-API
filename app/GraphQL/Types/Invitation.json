{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionProperties": false,
    "type": "object",
    "required": [
        "id",
        "organization_id",
        "role_id",
        "email",
        "used_at",
        "expired_at"
    ],
    "properties": {
        "id": {
            "$ref": "../types.json#/definitions/ID"
        },
        "organization_id": {
            "$ref": "../types.json#/definitions/ID"
        },
        "role_id": {
            "$ref": "../types.json#/definitions/ID"
        },
        "role": {
            "$ref": "./Role.json"
        },
        "email": {
            "type": "string"
        },
        "used_at": {
            "oneOf": [
                {
                    "type": "string",
                    "format": "date-time"
                },
                {
                    "type": "null"
                }
            ]
        },
        "expired_at": {
            "type": "string",
            "format": "date-time"
        }
    }
}
