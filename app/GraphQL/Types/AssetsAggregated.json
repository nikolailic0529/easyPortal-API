{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "count",
        "types",
        "coverages"
    ],
    "properties": {
        "count": {
            "type": "integer"
        },
        "types": {
            "type": "array",
            "items": {
                "$ref": "AssetsAggregatedType.json"
            }
        },
        "coverages": {
            "type": "array",
            "items": {
                "$ref": "AssetsAggregatedCoverage.json"
            }
        }
    }
}
