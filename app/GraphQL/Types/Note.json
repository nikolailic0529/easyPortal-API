{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionProperties": false,
    "type": "object",
    "required": [
        "created_at",
        "updated_at",
        "files",
        "note",
        "pinned",
        "user",
        "user_id",
        "id"
    ],
    "properties": {
        "id": {
            "$ref": "../types.json#/definitions/ID"
        },
        "user_id": {
            "$ref": "../types.json#/definitions/ID"
        },
        "note": {
            "type": "string"
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        },
        "user": {
            "$ref": "../Types/User.json"
        },
        "files": {
            "type": "array",
            "items": {
                "$ref": "../Types/File.json"
            }
        },
        "pinned" : {
            "type": [
                "boolean",
                "null"
            ]
        }
    }
}
